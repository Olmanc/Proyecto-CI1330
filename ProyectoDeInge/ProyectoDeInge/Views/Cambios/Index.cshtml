@model IEnumerable<ProyectoDeInge.Models.CAMBIOS>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Requerimiento
        </th>
        <th>
            Fecha de solicitud
        </th>
        <th>
            Solicitante
        </th>
        <th>
            Encargado de revisión
        </th>
        <th>
            Versión
        </th>
        <th>
            Estado
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FECHA)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.JUSTIFICACION)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.JUST_REV)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FECHA_REV)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.REQUERIMIENTOS.NOMBRE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.REQUERIMIENTOS1.NOMBRE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.USUARIOS.NOMBRE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.USUARIOS1.NOMBRE)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
